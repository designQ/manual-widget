{
  "name": "vuepress-web-app",
  "author": "nicejade",
  "version": "0.1.3",
  "description": "Web applications built with Vuepress",
  "scripts": {
    "install": "npm install",
    "install:cnpm": "cnpm install",
    "remove": "rimraf node_modules",
    "preview": "serve -s dist -p 9567",

    "start": "vuepress dev docs",
    "build": "vuepress build docs",
    "deploy": "bash deploy.sh",
    "prettier": "prettier --write '**/*.md'",
    "prettier-watch": "onchange '**/*.md' -- prettier --write {{changed}}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicejade/vuepress-web-app"
  },
  "dependencies": {
    "autosize": "^4.0.2",
    "axios": "^0.18.0",
    "date-fns": "^1.29.0",
    "element-ui": "^2.4.0",
    "github-markdown-css": "^2.10.0",
    "md5": "^2.2.1",
    "node-polyglot": "^2.3.0",
    "raw-loader": "^0.5.1"
  },
  "devDependencies": {
    "onchange": "^5.0.2",
    "prettier": "^1.14.3"
  },
  "lint-staged": {
    "**/**.{js,json,pcss,md,vue}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "printWidth": 120,
    "proseWrap": "never"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8"
  }
}
